!-*-{ Xft Settings
xft*dpi:    96
xft*antialias: True
xft*hinting: Full
!-*-}
!
!-*-{ URxvt
urxvt*font: xft:DejaVu Sans Mono:size=10
urxvt*urgentOnBell: True
urxvt*scrollBar: False
urxvt*secondaryscroll: true
urxvt*print-pipe: "cat > /dev/null"
urxvt*saveLines: 65535

urxvt*iso14755: False
urxvt*perl-ext-common: default,matcher,clipboard
urxvt*keysym.M-u: perl:url-select:select_next
urxvt*matcher.button: 1
urxvt*url-launcher: /usr/bin/chromium
urxvt*keysym.M-Escape: perl:keyboard-select:activate
urxvt*keysym.M-c: perl:clipboard:copy
urxvt*keysym.M-v: perl:clipboard:paste
urxvt*keysym.M-C-v: perl:clipboard_escaped
urxvt*copyCommand: xsel -ib
urxvt*pasteCommand: xsel -ob

urxvt*iconFile: /usr/share/icons/Faience/apps/scalable/utilities-terminal.svg
!Zenburn
urxvt*background: #3f3f3f
urxvt*foreground: #dcdccc
urxvt*cursorColor: #aaaaaa
urxvt*colorUL: #366060
urxvt*underlineColor: #dfaf8f
urxvt*color0: #3f3f3f
urxvt*color1: #cc9393
urxvt*color2: #7f9f7f
urxvt*color3: #d0bf8f
urxvt*color4: #6ca0a3
urxvt*color5: #dc8cc3
urxvt*color6: #93e0e3
urxvt*color7: #dcdccc
urxvt*color8: #000000
urxvt*color9: #dca3a3
urxvt*color10: #bfebbf
urxvt*color11: #f0dfaf
urxvt*color12: #8cd0d3
urxvt*color13: #dc8cc3
urxvt*color14: #93e0e3
urxvt*color15: #ffffff
!-*-}
