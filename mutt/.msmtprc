account kballou
host mail.runbox.com
port 587
protocol smtp
auth on
from kballou@devnulllabs.io
user kballou%devnulllabs.io
passwordeval "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.mail/kballou.devnulllabs.io.gpg"
tls on
tls_certcheck on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile ~/.cache/msmtp/msmtp.log

account homecu
host smtp.gmail.com
port 587
protocol smtp
auth on
from kenny@homecu.com
user kenny@homecu.com
passwordeval "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.mail/kenny.homecu.com.gpg"
tls on
tls_certcheck off
logfile ~/.cache/msmtp/homecu-msmtp.log

account default: kballou
